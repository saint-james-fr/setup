# Following are some useful aliases. Keep them, remove them, add your own. It's up to you!

# MetaScripts
alias cataliases="cat $HOME/.aliases"
alias editaliases="code $HOME/.aliases"
alias editzsh="code $HOME/.zshrc"

# System
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."
alias ......="cd ../../../../.."
alias .......="cd ../../../../../.."
alias ........="cd ../../../../../../.."
alias reload="exec zsh"